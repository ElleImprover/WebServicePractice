@{
}
@model APIPractice.ViewModels.TeacherViewModel
<h2>
    Meet a Random Teacher!
</h2>
<p>
    Meet @Model.Teacher.Name who is from @Model.Teacher.HomeState
</p>
<table style="padding:5px;">
    @foreach (Teacher t in Model.TeacherList)
    {
        <tr>
            <td>@t.Name</td>
            <td>@t.HomeState</td>
        </tr>
    }
</table>